<p id="notice"><%= notice %></p>

<p>
  <strong>User:</strong>
  <%= @list.user.name %>
</p>

<p>
  <strong>Name:</strong>
  <%= @list.name %>
</p>

<p>
  <strong>Did:</strong>
  <% @list.todos.each do |t| %>
    <% if t.done %>
     <li><%= link_to(t.name, todo_path(t.id)) %></li>
    <% end %>
  <% end %>
</p>


<%= link_to 'Back', list_path(@list) %>
